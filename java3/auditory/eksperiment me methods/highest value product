#include <iostream>
#include <cstring>
using namespace std;
class Product{
private:
    char name[100];
    float price;
    int quantity;
public:
    Product(){
    }
    Product(char *n, float p, int q){
        strcpy(name , n);
        price =p;
        quantity=q;
    }float totalvalue() {
        return price * quantity;
    }
    float getprice(){
        return price;
    }
    int getquantity(){
        return quantity;
    }
    void print(){
        cout<<name;
    }

};
class Order{
private:
    int ID;
    Product p[3];
public:
    Order(){
    }
    Order(int I, Product pp1 , Product pp2 ,Product pp3){
        ID=I;
        p[0]=pp1;
        p[1]=pp2;
        p[2]=pp3;
    }
    Product getp(int i){
        return p[i];
    }

};
Product highest(Order object){
    Product highest = object.getp(0);
    float totalValue= highest.totalvalue();
    for (int i = 0; i < 3; ++i) {
        Product current = object.getp(i);
        float currentValue=current.totalvalue();
        if (currentValue>totalValue){
            highest=current;
            totalValue=currentValue;
        }
    }
    return highest;
}
int main(){
    Product p1("Divella",200.0,30);
    Product p2("Dosa",130.0,40);
    Product p3("Bords",100.0,60);
    Order order1(23111, p1, p2,p3);
    Product highest1= highest(order1);
    highest1.print();
}
