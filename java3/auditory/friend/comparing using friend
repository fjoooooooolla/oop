#include <iostream>
#include <cstring>
using namespace std;
class Box{
private:
    int secret;
public:
    Box(int sekreti){
        secret=sekreti;
    }
    friend void findBiggest(Box boxes[], int n);
};
void findBiggest(Box boxes[], int current){
    int biggest=boxes[0].secret;
    for (int i = 1; i < 3; ++i) {
        int current=boxes[i].secret;
        if (current>biggest){
            biggest=current;
        }
    }
    cout<<biggest;


}

int main(){
    int n;
    Box secreti1(40);
    Box secreti2(60);
    Box secreti3(120);
    Box array[3]={secreti1, secreti2, secreti3};
    findBiggest(array, n);

};
